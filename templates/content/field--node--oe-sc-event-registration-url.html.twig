{#
/**
 * @file
 * Event registration button display.
 */
#}
{% set button %}
  {{ pattern('button', {
    'label': 'Register'|t,
    'icon': 'calendar-check',
    'icon_position': 'before',
  }) }}
{% endset %}
{% set _attributes = element[0]['#options']['attributes'] %}
{{ pattern('link', {
  'label': button,
  'path': element[0]['#url'].toString(),
  'attributes': _attributes,
}) }}


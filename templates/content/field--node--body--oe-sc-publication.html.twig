{#
/**
 * @file
 * Field template for publication long description.
 *
 * @see ./core/themes/stable/templates/field/field.html.twig
 */
#}
{% if items is not empty %}
  <h2 class="mb-4">{{ 'Description'|t }}</h2>
  <div class="mb-4-5">
    {% for item in items %}
      {{ item.content }}
    {% endfor %}
  </div>
{% endif %}


